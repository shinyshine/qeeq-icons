{
  "name": "icon-bin",
  "version": "1.0.1",
  "description": "transform svg files to react components, using bin command",
  "main": "index.js",
  "bin": {
    "icon-bin": "bin/icon-bin.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run copy",
    "copy": "copyfiles -u 1 src/tasks/templates/* src/tasks/templates/**/* lib",
    "g": "npm run generate",
    "generate": "cross-env NODE_ENV=production gulp --require ts-node/register/transpile-only",
    "test": "ts-node ./bin/icon-bin g-react-icons --config iconfont.config.js --baseDir ./global"
  },
  "keywords": [
    "typescript",
    "react",
    "svg",
    "icons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shinyshine/qeeq-icons.git"
  },
  "author": "lpp_kuku",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^4.3.5",
    "@types/gulp": "^4.0.9",
    "@rgrove/parse-xml": "^3.0.0",
    "@types/classnames": "^2.3.1",
    "@types/glob-stream": "^6.1.1",
    "@types/gulp-concat": "0.0.33",
    "@types/gulp-rename": "^2.0.1",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.template": "^4.5.0",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/node": "^16.6.1",
    "@types/ramda": "^0.27.44",
    "@types/react": "^17.0.17",
    "@types/rgrove__parse-xml": "^1.1.2",
    "@types/svgo": "^2.3.1",
    "@types/through2": "^2.0.36",
    "@types/yargs": "^17.0.2",
    "gulp-cli": "^2.3.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "classnames": "^2.3.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^2.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.template": "^4.5.0",
    "lodash.upperfirst": "^4.3.1",
    "pumpify": "^2.0.1",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "svgo": "^2.3.1",
    "through2": "^4.0.2",
    "ts-node": "^10.2.0",
    "webpack-iconfont-plugin-nodejs": "^1.0.19",
    "yargs": "^17.1.0"
  }
}
